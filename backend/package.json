{
  "name": "solar",
  "version": "1.0.0",
  "description": "**Goal:** Provide a search API that returns parts with Domestic vs Non-Domestic labels so FE can build UI now.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --runInBand",
    "seed": "node prisma/seed-from-xlsx.js",
    "reset-seed": "node -e \"const {PrismaClient}=require('@prisma/client');const p=new PrismaClient();(async()=>{await p.part.deleteMany();console.log('deleted all parts');await p.$disconnect();})()\" && node prisma/seed-from-xlsx.js",
    "seed:xlsx": "ts-node prisma/seed-from-xlsx.ts",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "feoc:vendor-template": "node scripts/export-feoc-vendor-template.js",
    "feoc:import-vendor": "node scripts/import-feoc-vendor-response.js",
    "frontend:serve": "cd frontend && python3 -m http.server 3001",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend:serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahharshbarger/Solar.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noahharshbarger/Solar/issues"
  },
  "homepage": "https://github.com/noahharshbarger/Solar#readme",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.12",
    "prom-client": "^15.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "concurrently": "^8.2.2",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
